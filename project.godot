config_version=5

[application]

config/name="YGBF"
run/main_scene="res://scenes/battle.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

GameManager="*res://scripts/singletons/game_manager.gd"
BattleManager="*res://scripts/singletons/battle_manager.gd"
